{
    "name": "grano22/test-kit",
    "description": "Test kit contains some utils to help you write your test in nice fashion",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.51",
        "phpunit/phpunit": "^10.3",
        "guzzlehttp/guzzle": "^7.4",
        "phpro/grumphp": "^2.10",
        "phpstan/phpstan": "^2.1",
        "spatie/phpunit-snapshot-assertions": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Grano22\\TestKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Grano22\\TestKit\\Tests\\Unit\\": "tests/unit",
            "Grano22\\TestKit\\Tests\\Kit\\": "tests/kit"
        }
    },
    "authors": [
        {
            "name": "Adrian BÅ‚asiak",
            "email": "blasiak.adrian@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true
        },
        "bin-dir": "bin"
    }
}
